{
  "name": "@dword-design/base",
  "version": "6.0.9",
  "description": "Base package for projects.",
  "keywords": [],
  "repository": "dword-design/base",
  "license": "MIT",
  "author": "Sebastian Landwehr <info@dword-design.de>",
  "main": "dist/index.js",
  "bin": {
    "base": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "commit": "rimraf dist && babel --config-file @dword-design/babel-config --out-dir dist --ignore \"**/*.spec.js\" src && node dist/cli.js commit",
    "depgraph": "rimraf dist && babel --config-file @dword-design/babel-config --out-dir dist --ignore \"**/*.spec.js\" src && node dist/cli.js depgraph",
    "prepare": "rimraf dist && babel --config-file @dword-design/babel-config --out-dir dist --ignore \"**/*.spec.js\" src && node dist/cli.js prepare",
    "prepublishOnly": "rimraf dist && babel --config-file @dword-design/babel-config --out-dir dist --ignore \"**/*.spec.js\" src && node dist/cli.js prepublishOnly",
    "release": "rimraf dist && babel --config-file @dword-design/babel-config --out-dir dist --ignore \"**/*.spec.js\" src && node dist/cli.js release",
    "test": "rimraf dist && babel --config-file @dword-design/babel-config --out-dir dist --ignore \"**/*.spec.js\" src && node dist/cli.js test"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/register": "^7.6.2",
    "@dword-design/babel-config": "^1.0.10",
    "@dword-design/base-config-node": "^1.0.0",
    "@dword-design/ci": "^1.0.1",
    "@dword-design/depcheck-config": "^1.1.0",
    "@dword-design/depgraph": "^1.0.0",
    "@dword-design/functions": "^2.2.2",
    "@dword-design/mocha-objects": "^1.0.0",
    "@dword-design/require-hook-test": "^1.0.0",
    "@qawolf/ci-info": "^2.1.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "ajv-cli": "^3.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "depcheck": "dword-design/depcheck#fork",
    "execa": "^4.0.0",
    "expect": "^25.1.0",
    "fs-extra": "^8.1.0",
    "get-package-name": "^1.0.0",
    "get-projectz-readme-section-regex": "^1.0.5",
    "glob-promise": "^3.4.0",
    "hosted-git-info": "^3.0.2",
    "ignore": "^5.1.4",
    "is-docker": "^2.0.0",
    "is-gitpod": "^1.0.7",
    "make-cli": "^1.0.9",
    "matchdep": "^2.0.0",
    "nyc": "^15.0.0",
    "output-files": "^1.1.7",
    "package-name-regex": "^1.0.5",
    "parse-git-config": "^3.0.0",
    "pre-commit": "^1.2.2",
    "resolve-from": "^5.0.0",
    "safe-readfile": "^1.0.2",
    "safe-require": "dword-design/node-safe-require#fork",
    "semantic-release": "^17.0.3",
    "sort-package-json": "^1.39.1",
    "spdx-expression-parse": "^3.0.0",
    "spdx-license-list": "^6.1.0",
    "stable-version-regex": "^1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "p-event": "^4.1.0",
    "port-ready": "^0.1.0",
    "rimraf": "^3.0.0",
    "stealthy-require": "^1.1.1",
    "tree-kill": "^1.2.2",
    "with-local-tmp-dir": "^2.1.2"
  },
  "baseConfig": {
    "depcheckConfig": {
      "ignoreMatches": [
        "pre-commit"
      ]
    }
  }
}
