    "depcheck-detector-package-name": "^2.0.0",
    "depcheck-package-name": "^2.0.0",
    "depcheck-parser-babel": "^2.0.0",
    "eslint": "~7.32.0",
    "execa": "^5.0.0",
    "find-up": "^5.0.0",
    "fs-extra": "^10.0.0",
    "get-projectz-readme-section-regex": "^3.0.0",
    "globby": "^11.0.1",
    "hosted-git-info": "^5.0.0",
    "husky": "^7.0.0",
