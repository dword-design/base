    "parse-packagejson-name": "^1.0.1",
    "plugin-name-to-package-name": "^0.1.1",
    "safe-readfile": "^1.0.2",
    "semantic-release": "^17.0.3",
    "sort-keys": "^4.2.0",
    "sort-package-json": "~1.57.0",
    "spdx-expression-parse": "^3.0.0",
    "spdx-license-list": "^6.1.0",
    "std-env": "^2.3.0",
    "yaml": "^1.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@dword-design/chdir": "^2.0.6",
