// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`index job matrix 1`] = `
Object {
  "jobs": Object {
    "cancel-existing": Object {
      "if": "!contains(github.event.head_commit.message, '[skip ci]')",
      "runs-on": "ubuntu-latest",
      "steps": Array [
        Object {
          "env": Object {
            "GITHUB_TOKEN": "\${{ secrets.GITHUB_TOKEN }}",
          },
          "uses": "rokroskar/workflow-run-cleanup-action@v0.3.3",
        },
      ],
    },
    "release": Object {
      "needs": "test",
      "runs-on": "ubuntu-latest",
      "steps": Array [
        Object {
          "uses": "actions/checkout@v4",
          "with": Object {
            "lfs": true,
            "ref": "\${{ github.event.pull_request.head.repo.full_name == github.repository && github.event.pull_request.head.ref || '' }}",
          },
        },
        Object {
          "uses": "actions/setup-node@v4",
          "with": Object {
            "node-version": 20,
          },
        },
        Object {
          "run": "corepack enable",
        },
        Object {
          "run": "git config --global user.email \\"actions@github.com\\"",
        },
        Object {
          "run": "git config --global user.name \\"GitHub Actions\\"",
        },
        Object {
          "run": "pnpm install --frozen-lockfile",
        },
        Object {
          "run": "pnpm checkUnknownFiles",
        },
        Object {
          "run": "pnpm lint",
        },
        Object {
          "env": Object {
            "GITHUB_REPOSITORY": "\${{ secrets.GITHUB_REPOSITORY }}",
            "GITHUB_TOKEN": "\${{ secrets.GITHUB_TOKEN }}",
          },
          "name": "Push changed files",
          "run": "pnpm dw-ci push-changed-files",
        },
        Object {
          "env": Object {
            "GITHUB_TOKEN": "\${{ secrets.GITHUB_TOKEN }}",
          },
          "if": "github.ref == 'refs/heads/master'",
          "name": "Release",
          "run": "pnpm semantic-release",
        },
      ],
    },
    "test": Object {
      "needs": "cancel-existing",
      "runs-on": "\${{ matrix.os }}",
      "steps": Array [
        Object {
          "uses": "actions/checkout@v4",
          "with": Object {
            "fetch-depth": 0,
            "lfs": true,
          },
        },
        Object {
          "uses": "actions/setup-node@v4",
          "with": Object {
            "check-latest": true,
            "node-version": "\${{ matrix.node }}",
          },
        },
        Object {
          "run": "corepack enable",
        },
        Object {
          "run": "pnpm install --frozen-lockfile",
        },
        Object {
          "env": Object {
            "GH_TOKEN": "\${{ secrets.GITHUB_TOKEN }}",
          },
          "run": "pnpm test",
        },
        Object {
          "if": "failure()",
          "uses": "actions/upload-artifact@v4",
          "with": Object {
            "name": "Image Snapshot Diffs",
            "path": "**/__image_snapshots__/__diff_output__",
          },
        },
        Object {
          "if": "matrix.os == 'ubuntu-latest' && matrix.node == 20",
          "uses": "codecov/codecov-action@v3",
          "with": Object {
            "token": "\${{ secrets.CODECOV_TOKEN }}",
          },
        },
      ],
      "strategy": Object {
        "matrix": Object {
          "include": Array [
            Object {
              "node": 18,
              "os": "ubuntu-latest",
            },
            Object {
              "node": 20,
              "os": "ubuntu-latest",
            },
            Object {
              "node": 20,
              "os": "macos-latest",
            },
            Object {
              "node": 20,
              "os": "windows-latest",
            },
          ],
        },
      },
    },
  },
  "name": "build",
  "on": Object {
    "pull_request": Object {},
    "push": Object {
      "branches": Array [
        "master",
      ],
    },
  },
}
`;

