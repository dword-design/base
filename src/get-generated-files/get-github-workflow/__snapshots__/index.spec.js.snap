            Object {
              "node": 14,
              "os": "ubuntu-latest",
            },
            Object {
              "node": 16,
              "os": "ubuntu-latest",
            },
            Object {
              "os": "macos-latest",
              "version": 16,
            },
            Object {
              "os": "windows-latest",
              "version": 16,
            },
          ],
        },
      },
    },
  },
